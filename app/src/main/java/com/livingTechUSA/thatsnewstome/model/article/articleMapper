import com.livingTechUSA.thatsnewstome.model.article.Articles
import com.livingTechUSA.thatsnewstome.model.article.DataCount

fun PagedArticleRaw.toDomain(): com.livingTechUSA.thatsnewstome.model.article.Articles =
    Articles(
        dataCountRaw.let {
            DataCount(
                it.pageCount,
                it.itemCount
            )
        },
        items.map {
            it.toDomain()
        }
    )

fun ArticleRaw.toDomain(): Article (
    source,
    author,
    title,
    description,
    url,
    urlToImage,
    publishedAt,
    content
)